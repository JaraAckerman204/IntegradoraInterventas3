{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ],
  "navigationRequestStrategy": "performance",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|webp|gif|ico|woff|woff2|ttf|eot)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "firebase-api",
      "urls": [
        "https://firestore.googleapis.com/**",
        "https://*.firebaseio.com/**",
        "https://firebase.googleapis.com/**",
        "https://firebasestorage.googleapis.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "performance",
        "timeout": "3s"
      }
    }
  ]
}