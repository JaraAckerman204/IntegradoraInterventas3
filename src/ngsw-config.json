{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|webp|ico)"
        ]
      }
    },
    {
      "name": "fonts",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "ionicons-cdn",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://cdn.jsdelivr.net/npm/ionicons@7.1.0/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-firestore",
      "urls": [
        "https://firestore.googleapis.com/**",
        "https://*.firebaseio.com/**",
        "https://identitytoolkit.googleapis.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "analytics",
      "urls": [
        "https://www.googletagmanager.com/**",
        "https://www.google-analytics.com/**"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1d",
        "strategy": "performance"
      },
      "cacheQueryOptions": {
        "ignoreSearch": true
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ],
  "navigationRequestStrategy": "freshness"
}